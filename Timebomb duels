local Players=game:GetService("Players")
local TweenService=game:GetService("TweenService")
local RunService=game:GetService("RunService")
local UserInputService=game:GetService("UserInputService")
local Workspace=game:GetService("Workspace")
local LocalPlayer=Players.LocalPlayer
local PlayerGui=LocalPlayer:WaitForChild("PlayerGui")
local Camera=Workspace.CurrentCamera

local function fast(i,p,t) return TweenService:Create(i,TweenInfo.new(t or 0.12,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),p) end
local function super(i,p,t) return TweenService:Create(i,TweenInfo.new(t or 0.06,Enum.EasingStyle.Linear,Enum.EasingDirection.Out),p) end

local function rootHum()
	local c=LocalPlayer.Character
	if not c then return end
	local r=c:FindFirstChild("HumanoidRootPart")
	local h=c:FindFirstChildOfClass("Humanoid")
	if r and h and h.Health>0 then return r,h end
end

local function rc(origin,dir,ignore)
	local rp=RaycastParams.new()
	rp.FilterType=Enum.RaycastFilterType.Blacklist
	rp.FilterDescendantsInstances=ignore or {LocalPlayer.Character}
	return Workspace:Raycast(origin,dir,rp)
end

local ScreenGui=Instance.new("ScreenGui")
ScreenGui.Name="لوحة_التحكم"
ScreenGui.IgnoreGuiInset=true
ScreenGui.ResetOnSpawn=false
ScreenGui.Parent=PlayerGui

local toggleBtn=Instance.new("ImageButton")
toggleBtn.Size=UDim2.new(0,60,0,60)
toggleBtn.Position=UDim2.new(0,12,0,12)
toggleBtn.BackgroundColor3=Color3.fromRGB(28,0,48)
toggleBtn.AutoButtonColor=true
toggleBtn.Image=""
toggleBtn.Parent=ScreenGui
local tCorner=Instance.new("UICorner",toggleBtn) tCorner.CornerRadius=UDim.new(0,14)
local tStroke=Instance.new("UIStroke",toggleBtn) tStroke.Color=Color3.fromRGB(200,0,255) tStroke.Thickness=2

local ok,thumb=pcall(function() return Players:GetUserThumbnailAsync(8415490735,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size100x100) end)
if ok and thumb then toggleBtn.Image=thumb else toggleBtn.Image="rbxassetid://8415490735" end

local Main=Instance.new("Frame")
Main.BackgroundColor3=Color3.fromRGB(24,0,44)
Main.Visible=false
Main.Parent=ScreenGui
local mCorner=Instance.new("UICorner",Main) mCorner.CornerRadius=UDim.new(0,18)
local mStroke=Instance.new("UIStroke",Main) mStroke.Color=Color3.fromRGB(190,0,255) mStroke.Thickness=2
local mGrad=Instance.new("UIGradient",Main)
mGrad.Color=ColorSequence.new(Color3.fromRGB(45,0,85),Color3.fromRGB(85,0,140))
mGrad.Rotation=30
local mScale=Instance.new("UIScale",Main) mScale.Scale=0

local Title=Instance.new("TextLabel")
Title.BackgroundTransparency=1
Title.Text="بواسطة: cryptonblox"
Title.TextColor3=Color3.fromRGB(255,255,255)
Title.Font=Enum.Font.GothamBold
Title.TextSize=20
Title.Size=UDim2.new(1,-20,0,32)
Title.Position=UDim2.new(0,10,0,8)
Title.Parent=Main

local Tabs=Instance.new("Frame")
Tabs.BackgroundTransparency=1
Tabs.Parent=Main

local function layout()
	local vp=Camera.ViewportSize
	local w=math.clamp(math.floor(vp.X*0.48),480,760)
	local h=math.clamp(math.floor(vp.Y*0.45),260,440)
	Main.Size=UDim2.new(0,w,0,h)
	Main.Position=UDim2.new(0,math.floor(vp.X*0.06),0,math.floor(vp.Y*0.10))
	Tabs.Size=UDim2.new(0,math.floor(w*0.30),1,-56)
	Tabs.Position=UDim2.new(0,10,0,56)
end
layout()
Camera:GetPropertyChangedSignal("ViewportSize"):Connect(layout)

local function makeTab(txt,idx,y)
	local b=Instance.new("TextButton")
	b.Text=txt
	b.Font=Enum.Font.GothamBold
	b.TextSize=18
	b.TextColor3=Color3.fromRGB(255,255,255)
	b.BackgroundColor3=Color3.fromRGB(90,0,150)
	b.AutoButtonColor=true
	b.Parent=Tabs
	b.Size=UDim2.new(1,0,0,46)
	b.Position=UDim2.new(0,0,0,y)
	local c=Instance.new("UICorner",b) c.CornerRadius=UDim.new(0,12)
	local s=Instance.new("UIStroke",b) s.Color=Color3.fromRGB(210,0,255) s.Thickness=1.5
	return b
end

local function makeScroll(parent)
	local sf=Instance.new("ScrollingFrame")
	sf.Parent=parent
	sf.BackgroundTransparency=1
	sf.ScrollBarImageColor3=Color3.fromRGB(190,0,255)
	sf.ScrollBarThickness=6
	sf.AutomaticCanvasSize=Enum.AutomaticSize.Y
	sf.ScrollingEnabled=true
	sf.Size=UDim2.new(1,-(Tabs.Size.X.Offset+30),1,-66)
	sf.Position=UDim2.new(0,Tabs.Position.X.Offset+Tabs.Size.X.Offset+10,0,56)
	local list=Instance.new("UIListLayout",sf)
	list.Padding=UDim.new(0,8)
	return sf
end

local Content1=makeScroll(Main)
local Content2=makeScroll(Main)
local Content3=makeScroll(Main)
Content2.Visible=false
Content3.Visible=false

local tab1=makeTab("عام • General",1,0)
local tab2=makeTab("استهداف • Target",2,54)
local tab3=makeTab("إضافات • Extras",3,108)

tab1.MouseButton1Click:Connect(function() Content1.Visible=true Content2.Visible=false Content3.Visible=false end)
tab2.MouseButton1Click:Connect(function() Content1.Visible=false Content2.Visible=true Content3.Visible=false end)
tab3.MouseButton1Click:Connect(function() Content1.Visible=false Content2.Visible=false Content3.Visible=true end)

local function makeRowButton(parent,text)
	local f=Instance.new("Frame")
	f.Parent=parent
	f.BackgroundTransparency=1
	f.Size=UDim2.new(1,0,0,46)
	local b=Instance.new("TextButton")
	b.Parent=f
	b.Text=text
	b.Font=Enum.Font.GothamBold
	b.TextSize=18
	b.TextColor3=Color3.fromRGB(255,255,255)
	b.BackgroundColor3=Color3.fromRGB(135,0,220)
	b.Size=UDim2.new(0.76,0,1,0)
	b.Position=UDim2.new(0,0,0,0)
	local c=Instance.new("UICorner",b) c.CornerRadius=UDim.new(0,12)
	local s=Instance.new("UIStroke",b) s.Color=Color3.fromRGB(240,120,255) s.Thickness=1.5
	local dot=Instance.new("Frame")
	dot.Parent=f
	dot.Size=UDim2.new(0,18,0,18)
	dot.Position=UDim2.new(0.78,0,0.5,-9)
	dot.BackgroundColor3=Color3.fromRGB(255,255,255)
	dot.BorderSizePixel=0
	local dc=Instance.new("UICorner",dot) dc.CornerRadius=UDim.new(1,0)
	return b,dot
end

local function makeTextBox(parent,ph)
	local tb=Instance.new("TextBox")
	tb.Parent=parent
	tb.Size=UDim2.new(0.76,0,0,44)
	tb.Text=""
	tb.PlaceholderText=ph
	tb.ClearTextOnFocus=false
	tb.Font=Enum.Font.GothamBold
	tb.TextSize=18
	tb.TextColor3=Color3.fromRGB(255,255,255)
	tb.BackgroundColor3=Color3.fromRGB(60,0,110)
	local c=Instance.new("UICorner",tb) c.CornerRadius=UDim.new(0,10)
	local s=Instance.new("UIStroke",tb) s.Color=Color3.fromRGB(200,0,255) s.Thickness=1.5
	return tb
end

local function setDot(dot,on) dot.BackgroundColor3=on and Color3.fromRGB(170,0,255) or Color3.fromRGB(255,255,255) end
local function pop(btn) btn:TweenSize(UDim2.new(0.73,0,1,0),"Out","Quad",0.08,true,function() btn:TweenSize(UDim2.new(0.76,0,1,0),"Out","Quad",0.08,true) end) end

local followBtn,followDot=makeRowButton(Content1,"ملاحقة أقرب لاعب • Follow")
