local function openPanel() Main.Visible=true fast(mScale,{Scale=1},0.12):Play() end
local function closePanel() local t=fast(mScale,{Scale=0},0.12) t:Play() t.Completed:Wait() Main.Visible=false end
local isOpen=false
toggleBtn.MouseButton1Click:Connect(function() if isOpen then closePanel() else openPanel() end isOpen=not isOpen end)

local drag=false local startPos local startUDim
local function enableDrag(gui)
	gui.InputBegan:Connect(function(i)
		if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then
			drag=true
			startPos=i.Position
			startUDim=gui.Position
		end
	end)
	gui.InputEnded:Connect(function(i)
		if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then drag=false end
	end)
	RunService.Heartbeat:Connect(function()
		if drag then
			local delta=UserInputService:GetMouseLocation()-startPos
			gui.Position=UDim2.new(startUDim.X.Scale,startUDim.X.Offset+delta.X,startUDim.Y.Scale,startUDim.Y.Offset+delta.Y)
		end
	end)
end
enableDrag(Main)
enableDrag(toggleBtn)

local followActive=false
local roofActive=false
local noclipActive=false
local skyActive=false
local specificFollow=false
local blockTools=false
local crazyActive=false
local roofOrig=nil
local skyOrig=nil
local roofY=nil

local function nearest(root)
	local best=nil local dBest=math.huge
	for _,p in ipairs(Players:GetPlayers()) do
		if p~=LocalPlayer and p.Character then
			local h=p.Character:FindFirstChildOfClass("Humanoid")
			local hrp=p.Character:FindFirstChild("HumanoidRootPart")
			if h and hrp and h.Health>0 and not p.Backpack:FindFirstChildOfClass("Tool") then
				local d=(root.Position-hrp.Position).Magnitude
				if d<dBest then dBest=d; best=p end
			end
		end
	end
	return best
end

local function findPrefix(t)
	if not t or #t<2 then return nil end
	t=t:lower()
	for _,p in ipairs(Players:GetPlayers()) do
		if p.Name:lower():sub(1,#t)==t then return p end
	end
end

followBtn.MouseButton1Click:Connect(function() pop(followBtn) followActive=not followActive setDot(followDot,followActive) end)
noclipBtn.MouseButton1Click:Connect(function() pop(noclipBtn) noclipActive=not noclipActive setDot(noclipDot,noclipActive) end)
skyBtn.MouseButton1Click:Connect(function()
	pop(skyBtn)
	local r=select(1,rootHum()) if not r then return end
	if not skyActive then skyOrig=r.CFrame r.Anchored=true fast(r,{CFrame=r.CFrame+Vector3.new(0,300,0)},0.12):Play() else r.Anchored=false r.CFrame=skyOrig or r.CFrame end
	skyActive=not skyActive setDot(skyDot,skyActive)
end)

local function placeUnderRoof(r)
	local up=rc(r.Position,Vector3.new(0,1000,0))
	if not up then return end
	roofOrig=r.CFrame
	roofY=up.Position.Y-2.2
	r.Anchored=true
	r.CFrame=CFrame.new(Vector3.new(r.Position.X,roofY,r.Position.Z))
end

roofBtn.MouseButton1Click:Connect(function()
	pop(roofBtn)
	local r=select(1,rootHum()) if not r then return end
	if not roofActive then
		placeUnderRoof(r)
	else
		if roofOrig then r.CFrame=roofOrig end
		r.Anchored=false
	end
	roofActive=not roofActive
	setDot(roofDot,roofActive)
end)

chaseBtn.MouseButton1Click:Connect(function() pop(chaseBtn) specificFollow=not specificFollow setDot(chaseDot,specificFollow) end)

nameBox:GetPropertyChangedSignal("Text"):Connect(function()
	local p=findPrefix(nameBox.Text)
	if p then local ok2,url=pcall(function() return Players:GetUserThumbnailAsync(p.UserId,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size100x100) end) if ok2 then headImg.Image=url end end
end)

tpBtn.MouseButton1Click:Connect(function()
	pop(tpBtn)
	local r=select(1,rootHum()) if not r then return end
	local p=findPrefix(nameBox.Text) if not p or not p.Character then return end
	local hrp=p.Character:FindFirstChild("HumanoidRootPart") if not hrp then return end
	local orig=r.CFrame
	fast(r,{CFrame=hrp.CFrame},0.12):Play()
	task.wait(1.2)
	fast(r,{CFrame=orig},0.12):Play()
end)

bangBtn.MouseButton1Click:Connect(function()
	pop(bangBtn)
	local r,h=rootHum() if not r then return end
	local p=findPrefix(nameBox.Text) if not p or not p.Character then return end
	local hrp=p.Character:FindFirstChild("HumanoidRootPart") if not hrp then return end
	h.Sit=true
	local front=hrp.CFrame*CFrame.new(0,0,-2.2)
	local back=r.CFrame
	super(r,{CFrame=front},0.05):Play()
	task.wait(0.1)
	super(r,{CFrame=back},0.05):Play()
	task.delay(0.2,function() if h then h.Sit=false end end)
end)

local speedVal=16
speedBox.FocusLost:Connect(function()
	local n=tonumber(speedBox.Text)
	if n and n>0 then speedVal=n end
end)

blockBtn.MouseButton1Click:Connect(function() pop(blockBtn) blockTools=not blockTools setDot(blockDot,blockTools) end)
crazyBtn.MouseButton1Click:Connect(function() pop(crazyBtn) crazyActive=not crazyActive setDot(crazyDot,crazyActive) end)

RunService.RenderStepped:Connect(function()
	local r,h=rootHum()
	if r and h then
		if h.WalkSpeed~=speedVal then h.WalkSpeed=speedVal end
		if followActive then
			local n=nearest(r)
			if n and n.Character and n.Character:FindFirstChild("HumanoidRootPart") then
				local cf=n.Character.HumanoidRootPart.CFrame*CFrame.new(0,3,0)
				r.CFrame=r.CFrame:Lerp(cf,0.28)
			end
		end
		if specificFollow then
			local t=findPrefix(nameBox.Text)
			if t and t.Character and t.Character:FindFirstChild("HumanoidRootPart") then
				local cf=t.Character.HumanoidRootPart.CFrame*CFrame.new(0,3,0)
				r.CFrame=r.CFrame:Lerp(cf,0.28)
			end
		end
		if noclipActive and LocalPlayer.Character then
			for _,p in ipairs(LocalPlayer.Character:GetChildren()) do if p:IsA("BasePart") then p.CanCollide=false end end
		elseif LocalPlayer.Character then
			for _,p in ipairs(LocalPlayer.Character:GetChildren()) do if p:IsA("BasePart") then p.CanCollide=true end end
		end
	end
end)

task.spawn(function()
	while true do
		task.wait(0.05)
		if blockTools then
			local ch=LocalPlayer.Character
			local bp=LocalPlayer:FindFirstChildOfClass("Backpack")
			if ch then for _,t in ipairs(ch:GetChildren()) do if t:IsA("Tool") then t:Destroy() end end end
			if bp then for _,t in ipairs(bp:GetChildren()) do if t:IsA("Tool") then t:Destroy() end end end
		end
		if roofActive then
			local r=select(1,rootHum())
			if r and roofY then
				r.Anchored=true
				local try=0
				local moved=false
				while try<4 and not moved do
					try+=1
					local off=Vector3.new(math.random(-6,6),0,math.random(-6,6))
					local target=Vector3.new(r.Position.X+off.X,roofY,r.Position.Z+off.Z)
					local toWall=rc(r.Position,(target-r.Position),{LocalPlayer.Character})
					if not toWall then
						super(r,{CFrame=CFrame.new(target)},0.05):Play()
						moved=true
					end
				end
			end
		end
		if crazyActive then
			local r=select(1,rootHum())
			if r then
				r.Anchored=true
				local seq={
					r.CFrame*CFrame.new(5,0,0),
					r.CFrame*CFrame.new(-10,0,0),
					r.CFrame*CFrame.new(0,6,0),
					r.CFrame*CFrame.new(0,-7,0)*CFrame.Angles(math.rad(360),0,0),
					r.CFrame*CFrame.new(6,2,0)*CFrame.Angles(0,math.rad(180),0),
				}
				for _,cf in ipairs(seq) do if not crazyActive then break end super(r,{CFrame=cf},0.06):Play() task.wait(0.07) end
				if not crazyActive then r.Anchored=false end
			end
		end
	end
end)

task.delay(0.05,function() Main.Visible=true fast(mScale,{Scale=1},0.12):Play() isOpen=true end)
